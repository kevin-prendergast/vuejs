{
  "name": "vue-js",
  "version": "1.0.0",
  "description": "simple vue js",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "dev": "webpack-dev-server --inline --hot --quiet --port 4000",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --history-api-fallback --port 4000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevin-prendergast/vuejs.git"
  },
  "keywords": [
    "vue"
  ],
